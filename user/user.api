import (
	"apis/signup.api"
)
type (
	LoginRequest {
		Name     string `json:"name"`
		Password string `json:"password"`
	}
	LoginResponse {
		Token string `json:"token"`
	}
)

@server(
	group: login
)
service user-api {
	@handler UserLoginHandler
	post /signin(LoginRequest) returns (LoginResponse)
}